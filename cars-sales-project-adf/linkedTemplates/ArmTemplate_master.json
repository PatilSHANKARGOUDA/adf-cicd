{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cars-sales-project-adf"
		},
		"ls_ADLSg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADLSg2'"
		},
		"ls_azure_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql_db'"
		},
		"ls_ADLSg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://carssalesprojectdl.dfs.core.windows.net/"
		},
		"ls_azure_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "carssalesserver.database.windows.net"
		},
		"ls_azure_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "carssalesdb"
		},
		"ls_azure_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminshankar"
		},
		"ls_github_carssales_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cars-sales-project-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ADLSg2_accountKey": {
						"value": "[parameters('ls_ADLSg2_accountKey')]"
					},
					"ls_azure_sql_db_password": {
						"value": "[parameters('ls_azure_sql_db_password')]"
					},
					"ls_ADLSg2_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLSg2_properties_typeProperties_url')]"
					},
					"ls_azure_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_server')]"
					},
					"ls_azure_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_database')]"
					},
					"ls_azure_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_userName')]"
					},
					"ls_github_carssales_properties_typeProperties_url": {
						"value": "[parameters('ls_github_carssales_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}